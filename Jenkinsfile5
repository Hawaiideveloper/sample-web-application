#!/usr/bin/env groovy

@Library('Jenkins-shared-library@master') _ //master or whatever branch



pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
		filterLogs ('WARNING', 1)
            }
        }
    }

